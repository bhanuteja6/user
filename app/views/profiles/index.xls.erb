<h1>Listing profiles</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Dob</th>
    <th>Gender</th>
    <th>Languages</th>
    <th>Picture</th>
    <th>File</th>
    <th>Email</th>
    <th>Aboutme</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%= profile.name %></td>
    <td><%= profile.dob %></td>
    <td><%= profile.gender %></td>
    <td><%= profile.languages %></td>
    <td><%= profile.picture %></td>
    <td><%= profile.file %></td>
    <td><%= profile.email %></td>
    <td><%= profile.aboutme %></td>
    
  </tr>
<% end %>
</table>

<br />
