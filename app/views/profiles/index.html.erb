<h1>Listing profiles</h1>
<%= will_paginate @profiles %>
<table>
  <tr>
    <th>Name</th>
    <th>Dob</th>
    <th>Gender</th>
    <th>Languages</th>
    <th>Picture</th>
    <th>File</th>
    <th>Email</th>
    <th>Aboutme</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%= profile.name %></td>
    <td><%= profile.dob %></td>
    <td><%= profile.gender %></td>
    <td><%= profile.languages %></td>
    <td><%= profile.picture %></td>
    <td><%= profile.file %></td>
    <td><%= profile.email %></td>
    <td><%= profile.aboutme %></td>
    <td><%= link_to 'Show', profile %></td>
    <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
    <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Profile', new_profile_path %>
<p>
  Download:
 
  <%= link_to "Excel", profiles_path(format: "xls") %>
</p>